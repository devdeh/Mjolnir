import mjlniscodecomplet as M

def data():
    choice1 = input("What do ya wanna do? Swipe or tap?")
    if choice1 == 'swipe':
        choice2 = input("Left, right, up or down?")
        if choice2 == 'up':
            M.choice(choice2)
        elif choice2 == 'down':
            M.choice(choice2)
        elif choice2 == 'left':
            M.choice(choice2)
        elif choice2 == 'right':
            M.choice(choice2)
    elif choice1 == 'tap':
        choice3 = input("You wanna tap, press or doubletap?")
        if choice3 == 'tap':
            M.choice(choice3)
        elif choice3 == 'press':
            M.choice(choice3)
        elif choice3 == 'doubletap':
            M.choice(choice3)

while True:
    go = input("Go or exit?")
    if go == 'go':
        data()
    elif go == 'exit':
        break

    
